{
	"name": "h5-logic-tool",
	"version": "1.0.3",
	"description": "h5-logic-tool",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.esm.js",
	"types": "./dist/types/index.d.ts",
	"files": [
		"dist"
	],
	"private": false,  	"scripts": {
		"log": "node -e require('./dist/index.cjs.js')",
		"dev": "rollup -w -c rollup.config.ts --configPlugin @rollup/plugin-typescript --environment BUILD_ENV:dev",
		"build:types": "tsc -b ./tsconfig.json",
		"build:rollup-dev": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript --environment BUILD_ENV:dev",
		"build:rollup-prod": "rollup -c rollup.config.ts --configPlugin @rollup/plugin-typescript --environment BUILD_ENV:prod",
		"build:dev": "npm run build:types && npm run build:rollup-dev",
		"build:prod": "npm run build:types && npm run build:rollup-prod",
		"prepublish": "pnpm version && pnpm build",
		"lint": "eslint 'src/**/*.{js,ts}'",
		"format": "prettier --write 'src/**/*.{js,ts,json,css}'"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/bigWhiteWhite/h5-logic-tool.git"
	},
	"keywords": [
		"utils"
	],
	"author": "bigWhiteWhite",
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"bugs": {
		"url": "https://github.com/bigWhiteWhite/h5-logic-tool/issues"
	},
	"homepage": "https://github.com/bigWhiteWhite/h5-logic-tool#readme",
	"dependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-typescript": "^11.1.6",
		"axios": "^0.27.2",
		"rollup": "^4.18.1",
		"rollup-plugin-cleanup": "^3.2.1",
		"rollup-plugin-sourcemaps": "^0.6.3",
		"rollup-plugin-terser": "^7.0.2",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3"
	},
	"devDependencies": {
		"@babel/core": "^7.24.9",
		"@babel/preset-env": "^7.24.8",
		"@rollup/plugin-alias": "^5.1.0",
		"@rollup/plugin-babel": "^6.0.4",
		"@rollup/plugin-eslint": "^9.0.5",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@types/node": "^20.14.11",
		"@types/rollup-plugin-json": "^3.0.7",
		"@types/rollup-plugin-node-builtins": "^2.1.5",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"prettier": "^3.3.3",
		"rimraf": "^6.0.1",
		"rollup-plugin-json": "^4.0.0",
		"rollup-plugin-node-builtins": "^2.1.2"
	}
}
